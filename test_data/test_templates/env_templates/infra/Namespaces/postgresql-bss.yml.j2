---
name: "{{ current_env.environmentName }}-postgresql-bss"
credentialsId: ""
labels:
- "solutionInstance-{{ current_env.environmentName }}"
- "solution-{{ current_env.tenant }}"
isServerSideMerge: false
cleanInstallApprovalRequired: true
mergeDeployParametersAndE2EParameters: false
deployParameters:
  dbaas:
    labels:
      bss_balancing_rule: postgresql-bss
      default_balancing_rule: postgresql
  postgresPassword: "${creds.get(\"postgresql-bss-cred\").password}"
  postgresUser: "${creds.get(\"postgresql-bss-cred\").username}"
e2eParameters: {}
technicalConfigurationParameters: {}
deployParameterSets:
  - postgresql-common
  - postgresql-dbaas
e2eParameterSets: []
technicalConfigurationParameterSets: []

